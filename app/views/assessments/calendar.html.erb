<h1>Calendar</h1>

<div id="calendar"></div>
<script>
$(document).ready(function() {
    $('#calendar').fullCalendar({
        header: {
            left: 'today',
            center: 'prev title next',
            right: 'month,agendaWeek,agendaDay'
        },
        defaultView :'month',
        height: 600,
        events: '/events.json'
    });
});
</script>
